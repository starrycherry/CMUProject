body {
	background: #555555;
}

.title {
	padding-top: 10px;
	padding-left: 1%;
	padding-right: 1%;
	background: #555555;
	height: 35px;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#555555', endColorstr='#3F3F3F');
	background: -webkit-gradient(linear, left top, left bottom, from(#555555), to(#3F3F3F));
	background: -moz-linear-gradient(top, #555555, #3F3F3F);
	width: 98%;
	text-align: center;
}

.logo {
	float: left;
}

.loginConfig {
	float: right;
	margin-top: 5px;
	margin-left: 2%;
	font-size: 15px;
	color: white;
}

.search {
	float: right;
	margin-top: 2px;
	margin-right: 1%;
}

.login, .logout, .welcome {
	font-size: 15px;
	color: white;
}

.preference {
	float: right;
	margin-top: 2px;
	margin-right: -1.3%;
}

a:link, a:hover, a:active, a:visited {
	text-decoration: none;
	color: white !important;
}

a:hover {
	text-decoration: underline;
	color: white !important;
}

.pageControl {
	float: right;
	margin-top: 3px;
	margin-right: 1%;
}

.paging {
	height: 23px;
	width: 100%;
	background: black;
	padding-top: 6px;
}

.innerBorder {
	background-image: url('../images/bg1.png');
	margin-left: auto;
	margin-right: auto;
	border-radius: 2px;
	width: 100%;
	height: 94%;
	border-color: #555555;
	border-width: 2px;
	border-style: solid;
	background-color: white;
	overflow:auto;
}

.innerBorder2 {
	background-image: url('../images/bg1.png');
	margin-left: auto;
	margin-right: auto;
	border-radius: 2px;
	width: 100%;
	height: 90%;
	border-color: #555555;
	border-width: 2px;
	border-style: solid;
	background-color: white;
	overflow:auto;
}

.config {
	position: absolute;
	top: 25px;
	right: 5%;
}

.configPanel {
	position: absolute;
	top: 20%;
	left: 39%;
	z-index: 999;
	width: 300px;
	background-color: white;
	border-radius: 5px;
	border-color: #C5C5C5;
	border-width: 1px;
	border-style: solid;
	box-shadow: 10px 10px 5px #888888;
}

.configPanel > div {
	font-family: Arial;
	font-size: 13px;
	color: white;
	border-top-left-radius: 5pt 5pt;
	border-top-right-radius: 5pt 5pt;
	height: 22px;
	background: #555555;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#555555', endColorstr='#3F3F3F');
	background: -webkit-gradient(linear, left top, left bottom, from(#555555), to(#3F3F3F));
	background: -moz-linear-gradient(top, #555555, #3F3F3F);
	border-color: #C5C5C5;
	padding-top: 6px;
	text-align: center;
}

.done {
	width: 80px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 30px;
}
.steps {
	margin: 20px auto;
	width: 200px;
}

.portal {
	min-height: 695px;
}

.pages {
	color: white;
	font-size: 15px;
	margin-right: 20px;
}

.nav {
	width: auto;
	float: right;
	margin-right: 30px;
}

.curPageInfo {
	color: white;
	font-size: 15px;
}

.labelheader {
	font-family:Arial; 
	font-size:20px;
	color:#c2c2c2;
}

.boxlabel {
	font-family:Arial;
	font-weight: bold;
	font-size: 14px;
	color:#002b37;
	margin-top:4px;
}

.z-comboitem {
	padding-bottom:4px;
}